    "license": "MIT",
    "type": "module",
    "main": "./dist/vimdb.js",
    "types": "./dist/vimdb.d.ts",
    "engines": {
        "node": ">=16.0.0"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "22.0.2",
        "@rollup/plugin-json": "4.1.0",
        "@rollup/plugin-node-resolve": "13.3.0",
        "@types/cheerio": "0.22.31",
        "@types/jest": "29.0.0",
        "@types/luxon": "3.0.1",
        "@types/node": "18.7.14",
        "@typescript-eslint/eslint-plugin": "5.36.1",
        "@typescript-eslint/parser": "5.36.1",
        "await-to-js": "3.0.0",
        "cheerio": "1.0.0-rc.12",
        "eslint": "8.23.0",
        "eslint-plugin-prettier": "4.2.1",
        "file-type": "16.5.3",
        "html-entities": "2.3.3",
        "jest": "29.0.1",
        "luxon": "3.0.3",
        "node-fetch": "3.2.10",
        "prettier": "2.7.1",
        "rollup": "2.79.0",
        "rollup-plugin-dts": "4.2.2",
        "rollup-plugin-terser": "7.0.2",
        "rollup-plugin-typescript2": "0.33.0",
        "ts-jest": "28.0.8",
        "ts-node": "10.9.1",
        "typedoc": "0.23.12",
        "typescript": "4.8.2"
    },
    "scripts": {
        "build": "rollup --config",
        "docs": "typedoc --options typedoc.json --disableSources",
