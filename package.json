{
    "name": "vimdb",
    "version": "4.0.7",
    "description": "Yet another IMDb Scraper",
    "author": "Vinicius Egidio <me@vinicius.io>",
    "license": "MIT",
    "type": "module",
    "main": "./dist/vimdb.js",
    "types": "./dist/vimdb.d.ts",
    "engines": {
        "node": ">=16.0.0"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "21.0.3",
        "@rollup/plugin-json": "4.1.0",
        "@rollup/plugin-node-resolve": "13.1.3",
        "@types/cheerio": "0.22.31",
        "@types/jest": "27.4.1",
        "@types/luxon": "2.3.1",
        "@types/node": "17.0.23",
        "@typescript-eslint/eslint-plugin": "5.17.0",
        "@typescript-eslint/parser": "5.17.0",
        "await-to-js": "3.0.0",
        "cheerio": "1.0.0-rc.10",
        "eslint": "8.12.0",
        "eslint-plugin-prettier": "4.0.0",
        "file-type": "16.5.3",
        "html-entities": "2.3.3",
        "jest": "27.5.1",
        "luxon": "2.3.1",
        "node-fetch": "3.2.3",
        "prettier": "2.6.1",
        "rollup": "2.70.1",
        "rollup-plugin-dts": "4.2.0",
        "rollup-plugin-terser": "7.0.2",
        "rollup-plugin-typescript2": "0.31.2",
        "ts-jest": "27.1.4",
        "ts-node": "10.7.0",
        "typedoc": "0.22.13",
        "typescript": "4.6.3"
    },
    "scripts": {
        "build": "rollup --config",
        "docs": "typedoc --options typedoc.json --disableSources",
        "lint": "eslint . --fix --ext .ts",
        "start": "node --loader ts-node/esm --es-module-specifier-resolution=node --trace-warnings src/index.ts",
        "test": "node --experimental-vm-modules node_modules/.bin/jest"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/vegidio/js-vimdb"
    },
    "keywords": [
        "imdb",
        "typescript",
        "scraper",
        "movies",
        "series"
    ]
}
