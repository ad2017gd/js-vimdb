        "@types/luxon": "2.0.4",
        "@types/node": "16.9.6",
        "@types/node-fetch": "2.5.12",
        "@typescript-eslint/eslint-plugin": "4.31.2",
        "@typescript-eslint/parser": "4.31.2",
        "await-to-js": "3.0.0",
        "cheerio": "1.0.0-rc.10",
        "eslint": "7.32.0",
        "eslint-plugin-prettier": "4.0.0",
        "file-type": "16.5.3",
        "html-entities": "2.3.2",
