{
    "name": "vimdb",
    "version": "4.0.4",
    "description": "Yet another IMDb Scraper",
    "author": "Vinicius Egidio <me@vinicius.io>",
    "license": "MIT",
    "type": "module",
    "main": "./dist/vimdb.js",
    "types": "./dist/vimdb.d.ts",
    "engines": {
        "node": ">=16.0.0"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "21.0.1",
        "@rollup/plugin-json": "4.1.0",
        "@rollup/plugin-node-resolve": "13.0.6",
        "@types/cheerio": "0.22.30",
        "@types/jest": "27.0.2",
        "@types/luxon": "2.0.7",
        "@types/node": "16.11.7",
        "@typescript-eslint/eslint-plugin": "5.3.1",
        "@typescript-eslint/parser": "5.3.1",
        "await-to-js": "3.0.0",
        "cheerio": "1.0.0-rc.10",
        "eslint": "8.2.0",
        "eslint-plugin-prettier": "4.0.0",
        "file-type": "16.5.3",
        "html-entities": "2.3.2",
        "jest": "27.3.1",
        "luxon": "2.1.1",
        "node-fetch": "3.1.0",
        "prettier": "2.4.1",
        "rollup": "2.60.0",
        "rollup-plugin-dts": "4.0.1",
        "rollup-plugin-terser": "7.0.2",
        "rollup-plugin-typescript2": "0.30.0",
        "ts-jest": "27.0.7",
        "ts-node": "10.4.0",
        "typedoc": "0.22.8",
        "typescript": "4.4.4"
    },
    "scripts": {
        "build": "rollup --config",
        "docs": "typedoc --options typedoc.json --disableSources",
        "lint": "eslint . --fix --ext .ts",
        "start": "node --loader ts-node/esm --experimental-specifier-resolution=node --trace-warnings src/index.ts",
        "test": "node --experimental-vm-modules node_modules/.bin/jest"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/vegidio/js-vimdb"
    },
    "keywords": [
        "imdb",
        "typescript",
        "scraper",
        "movies",
        "series"
    ]
}
